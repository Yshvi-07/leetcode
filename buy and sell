class Solution {
public:
    int maxProfit(vector<int>& prices) {
        int max=0;
        int d=0;
        int a=0;
        for(int i=0;i<prices.size();i++){
            if(prices[i]<prices[d])
                d=i;
            
                
                    
                
                max=prices[i]-prices[d];
                if(max>a)
                
                    a=max;
                
                 
            
        }
        return a;
    }
};
