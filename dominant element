#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int t;
	cin>>t;
	while(t--){
	    int n;
	    cin>>n;
	    int a[n];
	    int s=0;
	    int max=0;
	    int r=0;
	    for(int i=0;i<n;i++){
	        cin>>a[i];
	    }
	    for(int i=0;i<n;i++){
	        s=0;
	        for(int j=i+1;j<n;j++){
	            if(a[i]==a[j]){
	                s++;
	            }}
	        
	        
	        if(max<s){
	            
	            max=s;
	            r=2;
	        }
	        else if(max==s){
	            r=1;
	        }
	        
	    }
	    
	    
	    
	    
	    
	    if(r==2){
	        cout<<"YES"<<endl;
	    }
	    else if(r==1) {
	        cout<<"NO"<<endl;
	    }
	    
	}
	return 0;
}
