
#include <iostream>

using namespace std;

int main()
{
    int n,m;
    cin>>n>>m;
    int a[n][m];
    for(int i=0;i<n;i++){
        for(int j=0;j<m;j++){
            cin>>a[i][j];
        }
    }
    int s=0,r=0;
    for(int i=0;i<n;i++){
        for(int j=0;j<m;j++){
            s=s+a[i][j];
            
        }
        r=s;
        cout<<r<<endl;
        s=0;
    }
    return 0;
}
