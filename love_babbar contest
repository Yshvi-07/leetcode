include <bits/stdc++.h> 
#include<math.h>
int rakshaBandhan(vector < int > arr, int n) {
    // Write your code here.
    
   //return flag;
      
       //cout<<s<<endl;
    long long s=0;
    sort(arr.begin(),arr.end(),greater<int>());
    for(int i=0;i<n;i++){
        
            s=s+arr[i];
        if(s<=0){
            return i;
        }
    }
    //return n;
  
    }
