class Solution {
public:
    vector<int> smallerNumbersThanCurrent(vector<int>& nums) {
        int r=0;
        
        int s=nums.size();
        vector<int> a(s);
        for(int i=0;i<s;i++){
            r=0;
            for(int j=0;j<s;j++){
                if(nums[i]>nums[j]){
                    r++;
                }
                a[i]=r;
            }
        }
       return a;
    }
    
};
