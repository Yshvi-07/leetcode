class Solution {
public:
    int search(vector<int>& nums, int target) {
        int d=0;
        for(int i=0;i<nums.size();i++){
            if(nums[i]==target){
                d=1;
            }
            if(d!=0){
                return d;
            }
            
        }
       return 0; 
    }
};
